{
    "name": "michaelcozzolino/symfony-validation-enhancements-bundle",
    "type": "symfony-bundle",
    "version": "0.1.0",
    "autoload": {
        "psr-4": {
            "MichaelCozzolino\\SymfonyValidationEnhancementsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichaelCozzolino\\SymfonyValidationEnhancementsBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^7.1",
        "symfony/validator": "^7.1",
        "symfony/property-access": "^7.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^7.1.0",
        "vimeo/psalm": "dev-master",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^3.5"
    },
    "scripts": {
        "psalm": [
            "vendor/bin/psalm --no-cache"
        ]
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true,
            "pestphp/pest-plugin": true
        }
    }
}
